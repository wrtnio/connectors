{
  "$schema": "https://json.schemastore.org/commitlintrc.json",
  "extends": [
    "@commitlint/config-conventional"
  ],
  "rules": {
    "subject-case": [
      2,
      "always",
      [
        "lower-case",
        "sentence-case",
        "start-case",
        "upper-case"
      ]
    ]
  }
}
