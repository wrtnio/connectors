    }
  ],
  "info": {
    "title": "송장 조회",
    "version": "0.1.42",
    "description": "Starter kit of Nestia",
    "license": {
      "name": "AGPL-3.0-only"
    }
  },
  "paths": {
    "/connector/sweet-tacker/get-companies/recommended": {
      "post": {
        "tags": [
          "sweet-tracker"
        ],
        "parameters": [],
        "requestBody": {
          "description": "택배사 조회를 위한 조건",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ISweetTracker.IGetRecommendedCompanyListInput"
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "택배사 목록",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ISweetTracker.IGetRecommendedCompanyListOutput"
                }
              }
            }
          }
        },
        "summary": "송장 번호에 매칭되는 택배사 목록",
        "description": "송장번호에 매칭되는 택배사 목록을 조회합니다.",
        "x-wrtn-standalone": true
      }
    },
    "/connector/sweet-tacker/get-companies": {
      "post": {
        "tags": [
          "sweet-tracker"
        ],
        "parameters": [],
        "responses": {
          "201": {
            "description": "택배사 목록",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ISweetTracker.IGetCompanyListOutput"
                }
              }
            }
          }
        },
        "summary": "택배사 목록 조회",
        "description": "택배사 목록을 조회합니다.",
        "x-wrtn-standalone": true
      }
    },
    "/connector/sweet-tacker/tracking-info": {
      "post": {
        "tags": [
          "sweet-tracker"
        ],
        "parameters": [],
        "requestBody": {
          "description": "송장 번호를 조회하기 위한 조건",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ISweetTracker.IGetTrackingInfoInput"
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "택배의 이동 경로",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ISweetTracker.IGetTrackingInfoOutput"
                }
              }
            }
          }
        },
        "summary": "송장 조회",
        "description": "송장 번호를 조회합니다."
      }
    }
  },
  "components": {
    "schemas": {
      "ISweetTracker.IGetRecommendedCompanyListInput": {
        "type": "object",
        "properties": {
          "t_invoice": {
            "type": "string",
            "title": "송장번호."
          },
          "t_key": {
            "type": "string",
            "title": "sweet-tracker로부터 발급받은 키."
          }
        },
        "required": [
          "t_invoice",
          "t_key"
        ]
      },
      "ISweetTracker.IGetRecommendedCompanyListOutput": {
        "type": "object",
        "properties": {
          "Recommend": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/OmitISweetTracker.CompanyInternational"
            },
            "title": "택배사 목록."
          }
        },
        "required": [
          "Recommend"
        ],
        "title": "송장 번호에 매칭되는 택배사 목록 DTO."
      },
      "OmitISweetTracker.CompanyInternational": {
        "type": "object",
        "properties": {
          "Code": {
            "type": "string",
            "pattern": "^([+-]?\\d+(?:\\.\\d+)?(?:[eE][+-]?\\d+)?)$",
            "title": "택배사가 가지는 고유한 코드 값."
          },
          "Name": {
            "type": "string",
            "title": "택배사 이름."
          }
        },
        "required": [
          "Code",
          "Name"
        ],
        "description": "Construct a type with the properties of T except for those in type K."
      },
      "ISweetTracker.IGetCompanyListOutput": {
        "type": "object",
        "properties": {
          "Company": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ISweetTracker.Company"
            },
            "title": "택배사 목록."
          }
        },
        "required": [
          "Company"
        ],
        "title": "택배사 조회 응답 DTO."
      },
      "ISweetTracker.Company": {
        "type": "object",
        "properties": {
          "Code": {
            "type": "string",
            "pattern": "^([+-]?\\d+(?:\\.\\d+)?(?:[eE][+-]?\\d+)?)$",
            "title": "택배사가 가지는 고유한 코드 값."
          },
          "International": {
            "oneOf": [
              {
                "const": "false"
              },
              {
                "const": "true"
              }
            ],
            "title": "국제 배송 여부."
          },
          "Name": {
            "type": "string",
            "title": "택배사 이름."
          }
        },
        "required": [
          "Code",
          "International",
          "Name"
        ]
      },
      "ISweetTracker.IGetTrackingInfoInput": {
        "type": "object",
        "properties": {
          "t_invoice": {
            "type": "string",
            "title": "송장번호."
          },
          "t_key": {
            "type": "string",
            "title": "sweet-tracker로부터 발급받은 키."
          },
          "t_code": {
            "type": "string",
            "title": "택배사 코드.",
            "description": "택배사 코드는 택배사 조회를 할 때 나오는 `Code` 값을 대입해야 한다."
          }
        },
        "required": [
          "t_invoice",
          "t_key",
          "t_code"
        ]
      },
      "ISweetTracker.IGetTrackingInfoOutput": {
        "type": "object",
        "properties": {
          "senderName": {
            "type": "string",
            "title": "보내는 사람 이름."
          },
          "receiverAddr": {
            "type": "string",
            "title": "받는 사람 주소."
          },
          "firstDetail": {
            "$ref": "#/components/schemas/ISweetTracker.TrackingDetail",
