            "type": "string",
            "title": "배송 예정 시간.",
            "description": "'15시~17시' 형식의 텍스트."
          },
          "itemImage": {
            "type": "string",
            "title": "상품 이미지 URL."
          },
          "adUrl": {
            "type": "string",
            "title": "택배사에서 광고용으로 사용하는 주소."
          },
          "lastStateDetail": {
            "$ref": "#/components/schemas/ISweetTracker.TrackingDetail",
            "title": "lastStateDetail."
          },
          "zipCode": {
            "oneOf": [
              {
                "type": "null"
              },
              {
                "type": "string"
              }
            ],
            "title": "우편주소."
          },
          "invoiceNo": {
            "type": "string",
            "title": "운송장 번호."
          },
          "completeYN": {
            "oneOf": [
              {
                "$ref": "#/components/schemas/ConstantY__type"
              },
              {
                "$ref": "#/components/schemas/ConstantN__type"
              }
            ],
            "title": "배송 완료 여부."
          },
          "orderNumber": {
            "oneOf": [
              {
                "type": "null"
              },
              {
                "type": "string"
              }
            ],
            "title": "주문 번호."
          },
          "complete": {
            "type": "boolean",
            "title": "배송 완료 여부."
          },
          "recipient": {
            "type": "string",
            "title": "수령인 정보."
          },
          "receiverName": {
            "type": "string",
            "title": "받는 사람."
          },
          "result": {
            "type": "string",
            "title": "조회 결과."
          },
          "productInfo": {
            "oneOf": [
              {
                "type": "null"
              },
              {
                "type": "string"
              }
            ],
            "title": "상품 정보."
          },
          "itemName": {
            "type": "string",
            "title": "상품 이름."
          }
        },
        "required": [
          "senderName",
          "receiverAddr",
          "firstDetail",
          "level",
          "lastDetail",
          "estimate",
          "itemImage",
          "adUrl",
          "lastStateDetail",
          "zipCode",
          "invoiceNo",
          "completeYN",
          "orderNumber",
          "complete",
          "recipient",
          "receiverName",
          "result",
          "productInfo",
          "itemName"
        ]
      },
      "ISweetTracker.TrackingDetail": {
        "type": "object",
        "properties": {
          "code": {
            "oneOf": [
              {
                "type": "null"
              },
              {
                "type": "string"
              }
            ],
            "title": "배송 상태 코드"
          },
          "kind": {
            "type": "string",
            "title": "진행 상태."
