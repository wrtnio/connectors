      },
      "Constant3__type": {
        "const": 3,
        "title": "배송 중",
        "description": "배송 중"
      },
      "Constant4__type": {
        "const": 4,
        "title": "지점 도착",
        "description": "지점 도착"
      },
      "Constant5__type": {
        "const": 5,
        "title": "배송 출발",
        "description": "배송 출발"
      },
      "Constant6__type": {
        "const": 6,
        "title": "배송 완료",
        "description": "배송 완료"
      },
      "ConstantY__type": {
        "const": "Y",
        "title": "배송 완료.",
        "description": "배송이 완료된 경우에는 Y로 표기된다."
