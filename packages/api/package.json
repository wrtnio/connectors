{
  "private": false,
  "name": "@wrtnio/connector-api",
  "version": "0.1.431",
  "description": "SDK library generated by Nestia",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "npm run build:sdk && npm run compile",
    "build:sdk": "rimraf ../../src/api/functional && cd ../.. && npx nestia sdk && cd packages/api",
    "compile": "rimraf lib && tsc",
    "deploy": "cd ../.. && npm install && npm run build:api && cd ./packages/api && npm publish --access public",
    "prepare": "ts-patch install && typia patch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wrtnio/connectors"
  },
  "author": "Wrtn Technologies",
  "license": "AGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/wrtnio/connectors/issues"
  },
  "homepage": "https://github.com/wrtnio/connectors#readme",
  "files": [
    "lib",
    "swagger.json",
    "package.json",
    "README.md"
  ],
  "devDependencies": {
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.1",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@nestia/fetcher": "^4.0.2",
    "typia": "^7.0.1"
  }
}
